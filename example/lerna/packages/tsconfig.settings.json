{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "Node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "allowJs": false,
    "strict": true,
    "composite": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "**/*.d.ts"
  ]
}
